<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude specific files or directories from cloud backup -->
        <exclude domain="database" path="measurements.db" />
        <exclude domain="sharedpref" path="temp_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <!-- Include/exclude files for device-to-device transfer -->
        <include domain="sharedpref" path="user_settings.xml" />
    </device-transfer>
</data-extraction-rules>
